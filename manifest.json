{
  "manifest_version": 3,
  "name": "TTblocker",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }
  ],
  "permissions": ["scripting", "contextMenus", "tabs", "https://*/", "http://*/", "activeTab"],
  "background": {
    "service_worker": "background/background.js",
    "type": "module"
  }
}